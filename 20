#include <stdio.h>
#include <algorithm>
using namespace std;
int n, a[30000], i, t, b;
int main()
{
	scanf_s("%d", &n);
	for (i = 0; i < n; i++)
	{
		scanf_s("%d", &a[i]);
	}
	sort(a, a + n);
	scanf_s("%d", &t);
	for (i = 0; i < t; i++)
	{
		scanf_s("%d", &b);
		printf("%d\n", a[b - 1]);
	}
	return 0;
}
