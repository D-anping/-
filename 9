#include <stdio.h>
int n,i,j,a[101];
int main()
{
	scanf("%d",&n);
	for (i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	for (i=0;i<n;i++)
	{
		for (j=n-1;j>i;j--)
		{
			if (a[j]<a[j-1])
			{
				a[j]^=a[j-1]^=a[j]^=a[j-1];
			}
		}
	}
	for (i=0;i<n-1;i++)
	{
		if (a[i]==a[i+1])
		{
			for (j=i;j<n-1;j++)
			{
				a[j]=a[j+1];
			}
			i--;
			n--;
		}
	}
	printf("%d\n%d",n,a[0]);
	for (i=1;i<n;i++)
	{
		printf(" %d",a[i]);
	}
	printf("\n");
	return 0;
}
