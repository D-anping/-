int cmp(int k,int a)
{
	int i;
	if (k%a!=0)
	{
		for (i=0;i<4;i++)
		{
			if (k%10==a)
			{
				return 1;
			}
			else
			{
				k/=10;
			}
		}
		return 0;
	}
	else
	{
		return 0;
	}
}
#include <stdio.h>
int count,n,a,i;
long long sum;
int main()
{
	while (scanf("%d",&n)!=EOF)
	{
		while (n--)
		{
			scanf("%d",&a);
			sum=count=0;
			for(i=1000;i<=9999;i++)
			{
				if (cmp(i,a)==1)
				{
					sum+=i;
					count++;
				}
			}
			printf("%d %lld\n",count,sum);
		}
	}
	return 0;
}
