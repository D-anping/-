#include <stdio.h>
int n,t,k,a,i;
int main()
{
	while (scanf("%d",&n)!=EOF)
	{
		for (i=0;i<n;i++)
		{
			scanf("%d", &a);
			t = 1;
			k = 1;
			while (t%a!=0 && a%2!=0 && k<10000)
			{
				t=(t%a)*10+1;
				k++;
			}
			if (t%a==0 && a%2!=0 && k<10000)
			{
				printf("%d\n",k);
			}
		}
	}
	return 0;
}
